{
  "name": "wa-status-scheduler",
  "version": "1.0.0",
  "private": true,
  "description": "Dashboard lokal untuk schedule upload WhatsApp Status via Baileys",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "npm run build:ui && concurrently -n server,worker -c cyan,magenta \"npm:dev:server\" \"npm:dev:worker\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:worker": "tsx watch worker/index.ts",
    "build": "npm run build:ui && npm run build:ts",
    "build:ui": "node scripts/build-ui.mjs",
    "build:ts": "tsc -p tsconfig.json",
    "reset": "node scripts/reset-project.mjs",
    "start": "node dist/server/index.js",
    "worker": "node dist/worker/index.js",
    "test": "vitest run",
    "postinstall": "node scripts/build-ui.mjs"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "6.7.16",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.1",
    "framer-motion": "^12.23.24",
    "helmet": "^7.1.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "pino": "^9.3.2",
    "proper-lockfile": "^4.1.2",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.10",
    "concurrently": "^9.0.1",
    "esbuild": "^0.23.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
